{
  "name": "gt-explorer",
  "private": true,
  "version": "1.0.0",
  "description": "GT Explorer - A unified admin dashboard for Graston Technique data",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "start": "npm run start --workspace=frontend",
    "start:backend": "npm run start --workspace=backend",
    "lint": "npm run lint --workspace=frontend",
    "test": "npm run test --workspaces",
    "test:frontend": "npm run test --workspace=frontend",
    "test:backend": "npm run test --workspace=backend",
    "db:generate": "npm run db:generate --workspace=frontend",
    "db:push": "npm run db:push --workspace=frontend",
    "db:migrate": "npm run db:migrate --workspace=frontend",
    "db:studio": "npm run db:studio --workspace=frontend"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.3"
  }
}
